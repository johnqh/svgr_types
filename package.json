{
  "name": "@sudobility/svgr_types",
  "version": "1.0.0",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "bun run build:esm && bun run build:cjs",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json && bun run rename-cjs",
    "rename-cjs": "for f in dist-cjs/*.js; do mv \"$f\" \"${f%.js}.cjs\"; done && cp dist-cjs/*.cjs dist/ && cp dist-cjs/*.d.ts dist/ 2>/dev/null || true",
    "clean": "rm -rf dist dist-cjs",
    "prepublishOnly": "bun run clean && bun run build",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "peerDependencies": {
    "@sudobility/types": "^1.9.50"
  },
  "devDependencies": {
    "@sudobility/types": "^1.9.50",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  }
}
